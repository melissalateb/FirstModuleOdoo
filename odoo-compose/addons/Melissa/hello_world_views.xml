<?xml version="1.0" encoding="utf-8"?>
<odoo>
   <template id="hello_world_template">
        <div>
            <img t-attf-src="/{{module_name}}/static/src/img/icon.png" width="64" height="64" alt="Logo"/>
            <h1>Hello World!</h1>
        </div>
    </template>
    <data>
        <record id="view_hello_world_form" model="ir.ui.view">
            <field name="name">hello.world.form</field>
            <field name="model">hello.world</field>
            <field name="arch" type="xml">
                <form>
                    <field name="name"/>
                </form>
            </field>
        </record>
    </data>
</odoo>
